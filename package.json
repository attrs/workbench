{
  "name": "@attrs/workbench",
  "version": "0.6.0",
  "description": "Extensible Admin UI",
  "main": "lib/index.js",
  "homepage": "https://attrs.github.io/workbench/",
  "repository": "attrs/workbench",
  "author": "joje <joje.attrs@gmail.com>",
  "scripts": {
    "prepublishOnly": "npm run dist",
    "dev": "npm run watch",
    "dist": "rimraf lib && tsc -p tsconfig.json",
    "dist:css": "postcss src/**/*.css --base src --dir lib -m",
    "watch": "concurrently \"npm:watch:*\"",
    "watch:ts": "rimraf lib && tsc -w",
    "watch:css": "postcss src/**/*.css --base src --dir lib -m -w",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "license": "MIT",
  "dependencies": {
    "array-equal": "^1.0.0",
    "minimatch": "^3.0.4",
    "tinyselector": "^0.1.23"
  },
  "devDependencies": {
    "@types/node": "^12.12.7",
    "concurrently": "^5.2.0",
    "cssnano": "^4.1.10",
    "postcss-cli": "^7.1.1",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^8.0.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "stylelint": "^13.3.3",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.1",
    "typedoc": "^0.17.1",
    "typescript": "^3.8.3"
  },
  "files": [
    "lib",
    "dist",
    "LICENSE",
    "README.md"
  ]
}
